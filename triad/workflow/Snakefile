rule all:
	input:
		"../results/C.txt"

rule produceA:
	output:
		"../results/A.txt",
	shell:
		"date > {output}"

rule produceB:
	output:
		"../results/B.txt",
	shell:
		"date > {output}"

rule produceCFromAB:
	input:
		"../results/A.txt",
		"../results/B.txt",
	output:
		"../results/C.txt"
	shell:
		"cat {input} > {output}"

